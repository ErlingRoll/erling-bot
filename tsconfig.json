{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "module": "commonjs",
    "baseUrl": "./src",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "sourceMap": true,
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.d.ts"
  ],
  "esModuleInterop": true
}